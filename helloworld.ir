	int_literal 115
	int_literal 111
	int_literal 109
	int_literal 101
	int_literal 116
	int_literal 104
	int_literal 105
	int_literal 110
	int_literal 103

        # newline
	int_literal 10

        # nul ('\0') byte: string terminator
	int_literal 0
